    <section class="contact py-lg-5 py-md-5 py-3" id="contact">
        <div class="container">
            <div class="inner-sec-w3ls py-lg-5 py-3">
                <h4 class="sub-tittle-w3 text-uppercase text-center">İletişim</h4>
                <h3 class="tittle-w3 text-uppercase mb-lg-5 mb-3 text-center text-white">Bana Yazın</h3>

                <div class="contact_grid_right">
                    <form action="/Home/AddContact" method="post">
                        <div class="row contact_left_grid">
                            <div class="col-md-6 con-left" data-aos="fade-up">
                                <div class="form-group">

                                    <input class="form-control" type="text" name="UserName" placeholder="Adınız Soyadınız" required="">
                                </div>
                                <div class="form-group">

                                    <input class="form-control" type="email" name="UserMail" placeholder="Mail Adresiniz" required="">
                                </div>
                                <div class="form-group">

                                    <input class="form-control" type="text" name="Subject" placeholder="Konu" required="">
                                </div>
                            </div>
                            <div class="col-md-6 con-right" data-aos="fade-up">
                                <div class="form-group">
                                    <textarea id="textarea" name="Message" placeholder="Mesajınız" required=""></textarea>
                                </div>
                            </div>
                            <div class="sub-honey mx-auto mt-3 text-white">
                                @ViewBag.Message
                                <input class="form-control" type="submit" value="Gönder">
                            </div>
                        </div>
                    </form>
