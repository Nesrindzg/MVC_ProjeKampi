    <div class="modal fade" id="exampleModalCenter2" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header text-center">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="login px-4 mx-auto mw-100">
                        <h5 class="text-center mb-4">Yazar Hesabı Oluştur</h5>
                        @using (Html.BeginForm("AddWriter", "Writer", FormMethod.Post))
                        {

                            @Html.Hidden("WriterTitle", "Yeni Yazar")
                            @Html.Hidden("WriterAbout", "Hakkında bilgi yok")

                            <!-- Yazar Adı -->
                            <div class="form-group">
                                <label>Yazar Adı</label>
                                @Html.TextBoxFor(m => m.WriterName, new { @class = "form-control", placeholder = "Yazar adını girin", @required = "required" })
                                @Html.ValidationMessageFor(m => m.WriterName, "", new { @class = "text-danger" })
                            </div>

                            <!-- Yazar Soyadı -->
                            <div class="form-group">
                                <label>Yazar Soyadı</label>
                                @Html.TextBoxFor(m => m.WriterSurname, new { @class = "form-control required", placeholder = "Yazar soyadını girin", @required = "required" })
                                @Html.ValidationMessageFor(m => m.WriterSurname, "", new { @class = "text-danger" })
                            </div>

                            <!-- Yazar Mail -->
                            <div class="form-group">
                                <label>Yazar Mail</label>
                                @Html.TextBoxFor(m => m.WriterMail, new { @class = "form-control required", placeholder = "E-posta adresi girin", @required = "required" })
                                @Html.ValidationMessageFor(m => m.WriterMail, "", new { @class = "text-danger" })
                            </div>

                            <!-- Yazar Şifre -->
                            <div class="form-group">
                                <label>Yazar Şifresi</label>
                                @Html.PasswordFor(m => m.WriterPassword, new { @class = "form-control required", placeholder = "Şifre girin", @required = "required" })
                                @Html.ValidationMessageFor(m => m.WriterPassword, "", new { @class = "text-danger" })
                            </div>

                            <!-- Şifre Tekrar (sabit olarak bırakıldı) -->
                            <div class="form-group">
                                <label>Şifre Tekrar</label>
                                <input type="password" class="form-control" id="password2" placeholder="Şifrenizi tekrar girin" required="">
                            </div>

                            <button type="submit" class="btn btn-primary submit mb-4">Yazar Ekle</button>
                        }
                    </div>
                </div>

            </div>
        </div>
    </div>
